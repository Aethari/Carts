-- pico functions
function _update60()
	p.input()
	
	foreach(friends, function(f)
		f:update()
	end)
	
	b.update()
	foreach(enemies, function(e)
		e:update()
		
		if e.dead then
			del(enemies, e)
		end
	end)
	
	if p.inv then
		p.col = 5
	else
		p.col = 11
	end
end

function _draw()
	cls(0)

	map(0,0,0,0,17,17)

	b.draw()	
	foreach(friends, function(f)
		f:draw()
	end)

	p.draw()
	foreach(enemies, function(e)
		e:draw()
	end)
	
	-- boss attacks
	if b.draw2 then
		line(
			0,
			b.atkpos,
			(b.timer*5),
			b.atkpos,
			8
		)
	elseif b.draw3 then
		line(
			0,
			b.atkpos,
			(b.timer*5),
			b.atkpos,
			8
		)
		line(
			b.atkpos2,
			0,
			b.atkpos2,
			(b.timer*5),
			8
		)
	elseif b.draw4 then
		foreach(lasers, function(l)
			line(
				b.x,
				b.y,
				l.x,
				l.y,
				8
			)
		end)
	elseif b.draw5 then
	end
	
	-- boss health
	?"boss",20+b.mhp/4,111,7
	if b.hp > 0 then
		rectfill(
			26, 117,
			b.hp/2+26,
			121,
			8
		)
	end
	rect(
		26, 117,
		b.mhp/2+27,
		121,
		7
	)
	
	-- draw health
	?"♥",0,2,7
	if p.hp > 0 then
		rectfill(8,2,p.hp+8,6,8)
	end
	rect(7,2,p.mhp+9,6,7)
	
	-- draw stamina
	?"✽",0,10,7
	if p.st > 0 then
		rectfill(7,10,p.st+7,14,12)
	end
	rect(7,10,p.mst+7,14,7)
end
