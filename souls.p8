-- boss
b = {
	mhp = 150,
	hp = 150,
	
	x = 64,
	y = 35,
	
	xdir = rnd(2)-1,
	ydir = rnd(1),
	speed = .25,
	
	col = 8,
	
	-- whether or not the boss is
	-- attacking
	atk = false,
	canatk = false,
	atktimer = 0,
		
	-- a general purpose timer for
	-- whatever you need it for
	timer = 0,
	timermax = 0,
	runtimer = false
}

function b.draw()
	circ(b.x,b.y,8,b.col)
end

function b.update()
	local dx = b.xdir*b.speed
	local dy = b.ydir*b.speed
	
	-- left/right collision
	if
		col(b.x+10+dx, b.y+dy) or
		col(b.x-10+dx, b.y+dy)
	then
		dx *= -1
		b.xdir *= -1
	end
	
	-- up/down collision
	if
		col(b.x+dx, b.y+10+dy) or
		col(b.x+dx, b.y-10+dy)
	then
		dy *= -1
		b.ydir *= -1
	end
	
	b.x += dx
	b.y += dy

	if not b.atk then
		if b.atktimer >= 100 then
			b.canatk = true
		end
		b.atktimer += 1
	end
	
	if b.canatk then
		b.atk = true
		b.canatk = false

		local rand = rnd()
				
		if rand < .3 then
			local cnt = randr(3,5)
			
			for i=0,cnt do
				local en = ec:new(
					b.x-3,
					b.y-4
				)
				add(enemies, en)
			end
			
			b.atk = false
			b.atktimer = 0
		elseif rand < .5 then
			b.timermax = 40
			
			b.atkpos = randr(
				p.y-8,
				p.y+8
			)
			
			b.draw2 = true
			b.runtimer = true
			
			b.atk = false
			b.atktimer = 0
		elseif rand < .7 then
			b.timermax = 40
			
			b.atkpos = randr(
				p.y-8,
				p.y+8
			)
			
			b.atkpos2 = randr(
				p.x-8,
				p.x+8
			)
			
			b.draw3 = true
			b.runtimer = true
			
			b.atk = false
			b.atktimer = 0
		elseif rand < .9 then
			b.timermax = 40
			local cnt = randr(2,5)
			
			lasers = {}
			
			for i=1, cnt do
				add(lasers, {
					x = rnd(360),
					y = rnd(360)
				})
			end
			
			b.draw4 = true
			b.runtimer = true
			
			b.atk = false
			b.atktimer = 0
		elseif rand < 1 then
			b.timermax = 40
			
			local cnt = randr(3,5)
			
			for i=0,cnt do
				local en = ec:new(
					b.x-3,
					b.y-4
				)
				add(enemies, en)
			end

			b.atkpos = randr(
				p.y-8,
				p.y+8
			)
			
			b.atkpos2 = randr(
				p.x-8,
				p.x+8
			)

			cnt = randr(2,5)
			
			lasers = {}
			
			for i=1, cnt do
				add(lasers, {
					x = rnd(360),
					y = rnd(360)
				})
			end

			b.draw3 = true
			b.draw4 = true
			b.runtimer = true
		
			b.atk = false
			b.atktimer = 0
		end
	end
	
	if b.runtimer then
		if b.timer < b.timermax then
			b.timer += 1
		else
			b.timer = 0
			b.timermax = 0
			
			b.draw2 = false
			b.draw3 = false
			b.draw4 = false
			b.draw5 = false
		end
	end
end

