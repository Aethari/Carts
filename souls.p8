-- menu
m = {
	-- whether or not to update/
	-- draw the menu
	u = true,
	d = true,
	
	-- which menu item is selected
	pos = 1,
	itemcount = 6
}

function m.update()
	-- up/down
	if btnp(2) then
		if m.pos > 1 then
			m.pos -= 1
		else
			m.pos = m.itemcount
		end
	elseif btnp(3) then
		if m.pos < m.itemcount then
			m.pos += 1
		else
			m.pos = 1
		end
	end
	
	-- select
	if btnp(4) or btnp(5) then
		if m.pos == 1 then
			if p.xp >= p.hpcost then
				p.mhp += 2
				p.xp -= p.hpcost
				p.hpcost=flr(p.hpcost*1.5)
			end
		elseif m.pos == 2 then
			if p.xp >= p.stcost then
				p.mst += 1
				p.xp -= p.stcost
				p.stcost=flr(p.stcost*1.5)
			end
		elseif m.pos == 3 then
			if p.xp >= p.spdcost then
				p.maxspeed += .075
				p.xp -= p.spdcost
				p.spdcost=flr(p.spdcost*1.5)
			end
		elseif m.pos == 4 then
			if p.xp >= p.fccost then
				p.fc += 1
				p.xp -= p.fccost
				p.fccost=flr(p.fccost*1.5)
			end
		elseif m.pos == 5 then
			if p.xp >= p.dmgcost then
				p.dmg += 1
				p.xp -= p.dmgcost
				p.dmgcost=flr(p.dmgcost*1.5)
			end
		elseif 
			m.pos == m.itemcount
		then
			p.hp = p.mhp
			p.score = 0
			m.u = false
			m.d = false
		end
	end
end

function m.draw()
	cls(0)

	-- logo
	spr(77,52,5,3,3)
	
	-- hiscore display
