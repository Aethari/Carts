pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
-- untitled soulslike game
-- 2025 aethari
-- general library functions

-- checks a pixel for collision
function col(x, y)
	local mx = x/8
	local my = y/8
	return fget(mget(mx, my), 0)
end


-->8
-- player
local p = {
	x = 60,
	y = 60,
	dx = 0,
	dy = 0,

	sprite = 1,

	speed = .4,
	maxspeed = 1.5,
	
	atk = false,
	inv = false,
	
	mhp = 5,
	hp = 5,
	
	mst = 10,
	st = 10
}

function p.draw()
	spr(p.sprite, p.x, p.y)
end

function p.input()
	-- left/right
	if btn(0)	then
		if p.dx > -p.maxspeed then
			p.dx -= p.speed 
		end
	elseif btn(1)	then
		if p.dx < p.maxspeed then
			p.dx += p.speed
		end
	else
		if p.dx < 0 then
			p.dx += p.speed
		elseif p.dx > 0 then
			p.dx -= p.speed
		end
	end

	-- up/down
	if btn(2) then
		if p.dy > -p.maxspeed then
			p.dy -= p.speed
		end
	elseif btn(3) then
		if p.dy < p.maxspeed then
			p.dy += p.speed
		end
	else
		if p.dy < 0 then
			p.dy += p.speed
		elseif p.dy > 0 then
			p.dy -= p.speed
		end
	end
	
	-- left/right collision
	if
		col(p.x-1, p.y) or
		col(p.x-1, p.y+7)
	then
		if p.dx < 0 then
			p.dx = 0
		end
	elseif
		col(p.x+8, p.y) or
		col(p.x+8, p.y+7)
	then
		if p.dx > 0 then
			p.dx = 0
		end
	end
	
	-- up/down collision
	if
		col(p.x, p.y-1) or
		col(p.x+7, p.y-1)
	then
		if p.dy < 0 then
			p.dy = 0
		end
	elseif
		col(p.x, p.y+8) or
		col(p.x+7, p.y+8)
	then
		if p.dy > 0 then
			p.dy = 0
		end
	end

	if
		btn(5) and
		p.st > 0
	then
		p.inv = true
		p.st -= .475
	else
		p.inv = false
		if p.st < p.mst then
			p.st += .125
		end
	end
	
	if 
		btnp(4) and
		not p.inv
	then
		-- attack
		p.atk = true
	end

	p.x += p.dx
	p.y += p.dy
end

-->8
-- pico functions
function _update()
	p.input()
	
	if p.inv then
		p.sprite = 2
	else
		p.sprite = 1
	end
end

local atkframe = 32

function _draw()
	cls(0)

	map(0,0,0,0,17,17)

	p.draw()
	
	if p.atk then
		spr(atkframe,p.x,p.y-8)
		atkframe += .5
		p.atk = true
	end
	
	if atkframe > 36 then
		atkframe = 32
		p.atk = false
	end
	
	-- draw health
	?"♥",0,2,7
	if p.hp > 0 then
		rectfill(8,2,p.hp+8,6,8)
	end
	rect(7,2,p.mhp+9,6,7)
	
	-- draw stamina
	?"✽",0,10,7
	if p.st > 0 then
		rectfill(7,10,p.st+7,14,12)
	end
	rect(7,10,p.mst+7,14,7)
end
