-- enemy cube
ec = {}
ec.__index = ec

function ec:update()
	self.life -= 1
	
	if self.life <= 0 then
		self.dead = true
	end
	
	local dx = self.xdir*self.speed
	local dy = self.ydir*self.speed
	
	-- left/right collision
	if 
		col(
			self.x+7+dx,
			self.y+dy
		) or
		col(
			self.x-1+dx,
			self.y+dy
		)
	then
		dx *= -1
		self.xdir *= -1
	end
		
	-- up/down collision
	if
		col(
			self.x+dx,
			self.y-1+dy
		) or
		col(
			self.x+dx,
			self.y+7+dy
		)
	then
		dy *= -1
		self.ydir *= -1
	end
	
	self.x += dx
	self.y += dy
end

function ec:draw()
	local s = self.sprite
	local x = self.x
	local y = self.y
	
	spr(s, x, y)
end

function ec:new(x,y)
	local inst = {}
	setmetatable(inst, ec)
	
	inst.sprite = 2
	
	inst.x = x
	inst.y = y
	
	inst.speed = .9
	
	inst.xdir = rnd(2)-1
	inst.ydir = rnd(2)-1
	
	-- number of frames the cube
	-- will be alive
	inst.life = randr(180,250)
	inst.dead = false
	
	return inst
end

